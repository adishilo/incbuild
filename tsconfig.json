{
    "compilerOptions": {
        "alwaysStrict": true,
        "module": "commonjs",
        "strictNullChecks": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "target": "es2017",
        "outDir": "Output",
        "inlineSourceMap": true,
        "moduleResolution": "node",
        "skipLibCheck": true,
        "plugins": [
            {
                "name": "tslint-language-service"
            }
        ]
    },
    "exclude": [
        "node_modules",
        "Output"
    ]
}
